#include <stdio.h>
int main() {
  int i,num,isPrime=1;
  printf("enter a no:");
  scanf("%d",&num);
  if(num<=1 || num==0)
  {printf("%d is not a prime no.\n",num);
  return 0;}
  for(i=2;i<=num/2;i++){
      if(num%i==0){
          isPrime=0;
          break;
      }
  }
  if(isPrime==1)
  printf("%d is a prime no:.\n",num);
  else
  printf("%d is not a prime no:.\n",num);
    return 0;
}
