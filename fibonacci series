#include <stdio.h>
int main() {
    int n,i;
     int a=0,b=1,next;
     printf("enter the no of terms:");
     scanf("%d",&n);
     printf("fibonacci series:%d %d",a,b);
     for(i=3;i<=n;i++){
     next=a+b;
     printf("%d",next);
     a=b;
     b=next;
    
}
     printf("\n");
     return 0;
}
