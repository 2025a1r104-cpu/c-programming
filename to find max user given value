#include <stdio.h>
int main() {
    int n,i,num,max;
    printf("enter how many nos:");
    scanf("%d",&n);
    printf("enter no 1:");
    scanf("%d",&max);
    for(i=2;i<=n;i++){
        printf("enter no %d:",i);
        scanf("%d",&num);
        if (num>max)
        max=num;}
        printf("maximum no=%d",max);
     return 0;
}
